<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sale_order_wizard_view_form" model="ir.ui.view">
        <field name="name">sale.order.wizard.form</field>
        <field name="model">sale.order.from.file</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="partner_id"/>
                    <field name="barcode_file"/>
                </group>
                <footer>
                    <button string="Create Order" class="btn-primary" name="read_file" type="object"/>
                    <button string="Close" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>